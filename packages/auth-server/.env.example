# 0pflow Auth Server Environment Variables

# Nango secret key (0pflow's shared Nango account)
NANGO_SECRET_KEY=

# Tiger Cloud PostgreSQL connection string
DATABASE_URL=

# GitHub OAuth app credentials
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Public GitHub client ID (for browser-side OAuth redirect)
NEXT_PUBLIC_GITHUB_CLIENT_ID=

# DBOS Cloud deployment proxy (optional â€” only needed for `0pflow deploy`)
# Run: npx dbos-cloud login --get-refresh-token
# Then copy from ~/.dbos/credentials
DBOS_CLOUD_REFRESH_TOKEN=
DBOS_CLOUD_ORGANIZATION=
